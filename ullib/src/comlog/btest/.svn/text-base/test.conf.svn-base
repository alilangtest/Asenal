#日志等级
COMLOG_PROCNAME : test
COMLOG_LEVEL : 16
#支持的设备数目
COMLOG_DEVICE_NUM : 1
#支持的自定义等级
COMLOG_SELFDEFINE : xiaowei, hello
#支持的设备名
COMLOG_DEVICE0 : ULOG
COMLOG_DEVICE1 : UNET
COMLOG_DEVICE2 : TTY

#设备类型可选(FILE, NET, ULLOG, ULNET, TTY)
#FILE 	单文件输出
#ULLOG 	跟ullog一样分布的文件输出ULLOG
#NET 	向网络的一个文件输出
#ULNET	跟ullog一样分布的网络文件输出
#TTY 	向终端打印日志

#向本地文本以兼容ULLOG形式输出
ULOG_TYPE : FILE
#日志文件名
ULOG_NAME : test.log
#日志打印的路径
ULOG_PATH : ./log
#日志打印的格式
#ULOG_LAYOUT : {%L}-{%D}-{%P}-{%T}-{%N}-{%R}-{%U}
#接受的系统等级
ULOG_SYSLEVEL : 16
#是否开启日志打印
ULOG_OPEN : 1
#接受的自定义日志等级
ULOG_SELFLEVEL : xiaowei, hello
#日志单文件最大大小 单位M
ULOG_SIZE : 2000
#日志分割类型 0 表示不分割, 1表示按大小分割, 2表示按时间分割
ULOG_SPLITE_TYPE : 2
#每隔1分钟分割日志
ULOG_DATA_CUTTIME : 1


UNET_TYPE : NET
UNET_NAME : test.log
#配置ip端口和路径,可以配置多个做冗余
UNET_PATH : 10.23.1.163:9898/log 10.23.1.163:9898/log
UNET_OPEN : 1
UNET_SYSLEVEL : 16
UNET_SELFLEVEL : xiaowei, hello

TTY_TYPE : TTY
TTY_NAME : test.log
TTY_PATH : ./
TTY_OPEN : 1

