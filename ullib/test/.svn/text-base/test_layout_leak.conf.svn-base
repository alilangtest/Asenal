#进程名
COMLOG_PROCNAME : test_layout_leak
COMLOG_LEVEL : 16
#支持的设备数目
COMLOG_DEVICE_NUM : 3
#支持的自定义等级
COMLOG_SELFDEFINE : xiaowei, hello
#支持的设备名
COMLOG_DEVICE0 : FILE
COMLOG_DEVICE1 : AFILE
COMLOG_DEVICE2 : ULLOG

#device 0: FILE
FILE_TYPE : FILE
FILE_NAME : test_layout_leak_file.log
FILE_PATH : ./log
FILE_SYSLEVEL : 16
FILE_OPEN : 1
FILE_SELFLEVEL : xiaowei, hello
FILE_SIZE : 2000
FILE_LAYOUT : %L: %D: %P * %T %R

#device 1: AFILE
AFILE_TYPE : AFILE
AFILE_NAME : test_layout_leak_afile.log
AFILE_PATH : ./log
AFILE_SYSLEVEL : 16
AFILE_OPEN : 1
AFILE_SELFLEVEL : xiaowei, hello
AFILE_SIZE : 2000
AFILE_LAYOUT : %L: %D: %P * %T %R

#device 2: ULLOG
ULLOG_TYPE : ULLOG
ULLOG_NAME : test_layout_leak_ullog.log
ULLOG_PATH : ./log
ULLOG_SYSLEVEL : 16
ULLOG_OPEN : 1
ULLOG_SELFLEVEL : xiaowei, hello
ULLOG_SIZE : 2000
ULLOG_LAYOUT : %L: %D: %P * %T %R
